<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.MainMenuController">
   <children>
      <Label id="title" text="Welcome to Physics Reviewer App!" textAlignment="JUSTIFY" wrapText="true">
         <font>
            <Font name="Century Gothic Bold" size="50.0" />
         </font>
         <VBox.margin>
            <Insets bottom="100.0" />
         </VBox.margin></Label>
      <GridPane alignment="TOP_CENTER" prefHeight="260.0" prefWidth="1312.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="200" minWidth="25.0" prefWidth="341.6" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="200" minWidth="25.0" prefWidth="498.9999999999999" />
           <ColumnConstraints hgrow="SOMETIMES" maxWidth="200" minWidth="25.0" prefWidth="758.5999999999999" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="200" prefHeight="200" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="lessonButton" alignment="CENTER" onAction="#switchToLessonMenu" text="Review the Lessons" textAlignment="CENTER" translateX="37.0" GridPane.rowIndex="1">
               <font>
                  <Font name="Century Gothic Bold" size="12.0" />
               </font></Button>
            <Button fx:id="quizButton" onAction="#switchToQuizMenu" text="Take the Quizzes" translateX="47.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font name="Century Gothic Bold" size="12.0" />
               </font>
            </Button>
             <ImageView fitHeight="200.0" fitWidth="190.0" translateX="4.0" GridPane.columnIndex="2">
                 <image>
                     <Image url="@../../exitImage.jpg" />
                 </image>
             </ImageView>
            <Button fx:id="exitButton" onAction="#exit" text="Exit" translateX="81.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <font>
                  <Font name="Century Gothic Bold" size="12.0" />
               </font>
            </Button>
            <ImageView fitHeight="200.0" fitWidth="178.0" translateX="11.0" GridPane.columnIndex="1">
               <image>
                  <Image url="@../../quizImage.jpg" />
               </image>
            </ImageView>
            <ImageView fitHeight="200.0" fitWidth="190.0" translateX="4.0">
               <image>
                  <Image url="@../../reviewImage.jpg" />
               </image>
            </ImageView>
         </children>
      </GridPane>
      <Label />
      <Label />
      <Label />
      <Label />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
